class Val {
    
    Bool equalsZero(Val self) {
        return self as Bool;
    }

    Val input() {
        System.setRegister<N0>(2);
        return System.getRegister<N2>();
    }

    Bool equals(Val self, Val other) {
        Val copy = self;
        Val copy1 = other;
        loop {
            if copy.equalsZero() {
                return copy1.equalsZero();
            };
            copy.dec();
            copy1.dec();
        };
    }

    Bool greater(Val self, Val other) {
        Val copy = self;
        Val copy1 = other;
        loop {
            if copy.equalsZero() {
                return Bool.false();
            } else if copy1.equalsZero() {
                return Bool.true();
            };
            copy.dec();
            copy1.dec();
        };
    }

    sub(Val self, Val other) {
        Val g = other;
        loop {
            if other.equalsZero() {
                break;
            };
            self.dec();
            other.dec();
        }
    }

    printDec(Val self) {
        if self.greater(9) {
            '1'.print();
            Val k = self;
            k.sub(10);
            k.print();
        } else {
            self.print();
        };
    }

    print(Val self) {
        System.setRegister<N1>(3);
        System.setRegister<N2>(self);
        System.setRegister<N0>(1);
    }

    inc(Val self) {}
    dec(Val self) {}
}