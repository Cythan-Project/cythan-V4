class DynArray<T, N> {

    Array<T, N> array;
    Val length;

    Self new() {
        return Self {
            array = Array tmp,
            length = 0
        };
    }
    
    Self from<Number>(Array<T, Number> input) {
        Self dyn = Self.new();
        dyn.addAll<Number>(input);
        return dyn;
    }

    add(Self self, T t) {
        self.array.setDyn(self.length, t);
        self.length.inc();
    }

    addAll<Ng>(Self self, Array<T, Ng> arr) {
        Val l = arr.len();
        Val c = 0;
        loop {
            if l.equalsZero() {
                break;
            };
            self.add(arr.getDyn(c));
            c.inc();
            l.dec();
        };
    }

    T pop(Self self) {
        self.length.dec();
        return self.getDyn(self.length);
    }

    Array<T, N> raw(Self self) {
        return self.array;
    }

    Val len(Self self) {
        return self.length;
    }

    Val capacity(Self self) {
        return self.array.len();
    }

    last(Self self, Val pos) {
        Val k = self.len();
        k.dec();
        return self.getDyn(k);
    }

    setDyn(Self self, Val pos, T t) {
        self.array.setDyn(pos,t);
    }

    set<Number>(Self self,T t) {
        self.set<Number>(t);
    }

    T getDyn(Self self, Val pos) {
        return self.array.getDyn(pos);
    }

    T get<Number>(Self self) {
        return self.array.get<Number>();
    }
    
    print(Self<Byte, N> self) {
        Val index = self.len();
        loop {
            index.dec();
            self.getDyn(index).print();
            if index.equalsZero() {
                break;
            };
        };
    }
    println(Self<Byte, N> self) {
        Val index = 0;
        loop {
            if self.len().greater(index) {
                self.getDyn(index).print();
                index.inc();
            } else {
                break;
            };
        };
        '\n'.print();
    }

    Bool contains(Self self, T t) {
        Val size = self.len();
        loop {
            if size.equalsZero() {
                return false;
            };
            size.dec();
            if self.getDyn(size).equals(t) {
                return true;
            } 
        };
    }
}