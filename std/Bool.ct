@AllowOnly(0..=1)
class Bool {

    Val value;

    Bool true() {
        return Bool { value = 0 };
    }

    Bool false() {
        return Bool { value = 1 };
    }
    Bool not(Bool self) {
        return if self.value as Bool {
            Bool.false()
        } else {
            Bool.true()
        };
    }

    print(Bool self) {
        if self {
            "true".print();
        } else {
            "false".print();
        }
    }
}